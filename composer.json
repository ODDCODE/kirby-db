{
    "name": "beebmx/kirby-db",
    "description": "Enable Kirby 3 database support for Illuminate\\Database",
    "keywords": [
        "kirby", "kirby-3", "database", "model", "eloquent"
    ],
    "version": "1.0.0",
    "type": "kirby-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Fernando Gutierrez",
            "email": "fernando@beeb.mx"
        }
    ],
    "require": {
        "composer/installers": "~1.0"
    },
    "require-dev": {
        "illuminate/database": "5.5.*"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "files": [
            "config.php"
        ],
        "psr-4": {
            "Beebmx\\": "src/"
        }
    },
    "minimum-stability": "dev"
}
